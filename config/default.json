{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "db": {
    "host": "localhost",
    "user": "postgres",
    "password": "postgres",
    "database": "sync-storage",
    "port": 5432,
    "enableSslAuth": false,
    "rejectUnauthorized": true,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  }
}
